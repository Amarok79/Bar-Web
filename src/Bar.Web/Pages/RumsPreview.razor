<p class="preview-title">Unsere Rums</p>

<div class="item-container">
    @foreach (var item in mItems) {
        <Card Image="@(Urls.GetLowResRumImage(item.Image))"
              SubTitle="@item.Teaser"
              Title="@item.Name">
        </Card>
    }
</div>

<a class="btn btn-dark"
   href="@Urls.Rums"
   style="margin: 20px 0 0 0">
    Alle Rums
</a>
